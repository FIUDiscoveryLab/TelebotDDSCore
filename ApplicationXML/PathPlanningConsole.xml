<?xml version="1.0"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="~/RTI/ndds.5.1.0/resource/qos_profiles_5.1.0/schema/rti_dds_profiles.xsd" version="5.1.0">

  <!-- Participant library used for the PathPlanningConsole application -->
  <participant_library name="PathPlanningConsoleLib">
    
    <!-- Participants -->
    <domain_participant name="PathPlanningConsole" domain_id="0">
      
      <!-- Participant QoS -->
      <participant_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
      
      <!-- Type registrations -->
      <register_type name="TVehicleCommand" kind="userGenerated" />
      <register_type name="TVehicleState" kind="userGenerated" />
      <register_type name="TWaypointCourse" kind="userGenerated" />
      <register_type name="TWaypoint" kind="userGenerated" />
      <register_type name="TPathPlanningRequest" kind="userGenerated" />
      
      <!-- Topics -->
      <topic name="VehicleCommandRequested" register_type_ref="TVehicleCommand" />
      <topic name="VehicleCommandCurrent" register_type_ref="TVehicleCommand" />
      
      <topic name="PathPlanningRequested" register_type_ref="TPathPlanningRequest" />
      <topic name="PathPlanningCurrent" register_type_ref="TPathPlanningRequest" />
      
      <topic name="VehicleState" register_type_ref="TVehicleState" />
      <topic name="PIDTargetWaypointCurrent" register_type_ref="TWaypoint" />
      <topic name="WaypointCourseCurrent" register_type_ref="TWaypointCourse" />
      
      
      <!-- Publisher -->
      <publisher name="MyPublisher">
	
	<!-- Publisher QoS -->
	<publisher_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	
	<!-- Data Writers -->
	<data_writer name="VehicleCommandWriter" topic_ref="VehicleCommandRequested">
	  <datawriter_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_writer>
	
	<data_writer name="PathPlanningRequestWriter" topic_ref="PathPlanningRequested">
	  <datawriter_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_writer>
      
      </publisher>
      
      <!-- Subscriber -->
      <subscriber name="MySubscriber">
	
	<!-- Subscriber QoS -->
	<subscriber_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	
	<!-- Data Readers -->
	<data_reader name="VehicleCommandReader" topic_ref="VehicleCommandCurrent">
	  <datarader_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_reader>
	
	<data_reader name="PathPlanningReader" topic_ref="PathPlanningCurrent">
	  <datarader_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_reader>
	
	<data_reader name="VehicleStateReader" topic_ref="VehicleState">
	  <datarader_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_reader>
	
	<data_reader name="WaypointReader" topic_ref="PIDTargetWaypointCurrent">
	  <datarader_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_reader>
	
	<data_reader name="WaypointCourseReader" topic_ref="WaypointCourseCurrent">
	  <datarader_qos base_name="BasicAEONProfile::MaxTransportThroughput" />
	</data_reader>
	
      </subscriber>
      
    </domain_participant>
    
  </participant_library>  
    
</dds>
